                                        <fo:block-container space-after="1em" space-before="1em"><fo:block end-indent="0" start-indent="0"><fo:inline font-weight="bold">Related
                                                concepts</fo:inline><fo:block><fo:inline/></fo:block><fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="1em" space-before="1.33em">
                                                <fo:list-item space-after="1em"><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol">•</fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:inline color="blue" text-decoration="underline"><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_15">Deploying IBM
                                                  StoredIQ</fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_15"><fo:inline font-style="italic" text-decoration="underline"><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_15"> (on page </fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_15"><fo:page-number-citation ref-id="unique_15"/></fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_15">)</fo:basic-link></fo:inline></fo:basic-link></fo:inline></fo:block></fo:list-item-body></fo:list-item>
                                            </fo:list-block></fo:block></fo:block-container>
