            <prodname class="- topic/prodname " xtrc="prodname:1;8:23" xtrf="file:/C:/Users/patscully/Dropbox/Current%20Working%20Clients/Information%20Designs%20&amp;%20Development/project_2_ps/workspace/publications/deployment_and_configuration_guide/deployment_and_configuration_guide.ditamap">IBM StoredIQ</prodname>
