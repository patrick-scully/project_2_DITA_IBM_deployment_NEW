        <glossdef class="- topic/abstract concept/abstract glossentry/glossdef " xtrc="glossdef:2;32:19" xtrf="file:/C:/Users/patscully/Dropbox/Current%20Working%20Clients/Information%20Designs%20&amp;%20Development/project_2_ps/workspace/publications/deployment_and_configuration_guide/topics/glossary/deployment_glossary.dita">Automated document categorization</glossdef><prolog class="- topic/prolog "><metadata class="- topic/metadata "><prodinfo class="- topic/prodinfo " xtrc="prodinfo:1;7:19" xtrf="file:/C:/Users/patscully/Dropbox/Current%20Working%20Clients/Information%20Designs%20&amp;%20Development/project_2_ps/workspace/publications/deployment_and_configuration_guide/deployment_and_configuration_guide.ditamap">
