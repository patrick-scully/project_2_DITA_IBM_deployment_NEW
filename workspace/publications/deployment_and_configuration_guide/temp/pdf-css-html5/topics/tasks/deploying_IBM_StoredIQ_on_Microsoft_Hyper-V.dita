                <tutorialinfo class="- topic/itemgroup task/tutorialinfo " xtrc="tutorialinfo:1;68:31" xtrf="file:/C:/Users/patscully/Dropbox/Current%20Working%20Clients/Information%20Designs%20&amp;%20Development/project_2_ps/workspace/publications/deployment_and_configuration_guide/topics/tasks/deploying_IBM_StoredIQ_on_Microsoft_Hyper-V.dita">
                    <sl class="- topic/sl " xtrc="sl:1;69:25" xtrf="file:/C:/Users/patscully/Dropbox/Current%20Working%20Clients/Information%20Designs%20&amp;%20Development/project_2_ps/workspace/publications/deployment_and_configuration_guide/topics/tasks/deploying_IBM_StoredIQ_on_Microsoft_Hyper-V.dita">
                        <sli class="- topic/sli " xtrc="sli:1;70:30" xtrf="file:/C:/Users/patscully/Dropbox/Current%20Working%20Clients/Information%20Designs%20&amp;%20Development/project_2_ps/workspace/publications/deployment_and_configuration_guide/topics/tasks/deploying_IBM_StoredIQ_on_Microsoft_Hyper-V.dita"><xref class="- topic/xref " format="html" href="https://docs.microsoft.com/en-us/previous-versions/windows/it-pro/windows-server-2012-R2-and-2012/dn874008(v=ws.11)?redirectedfrom=MSDN" scope="external" xtrc="xref:1;72:60" xtrf="file:/C:/Users/patscully/Dropbox/Current%20Working%20Clients/Information%20Designs%20&amp;%20Development/project_2_ps/workspace/publications/deployment_and_configuration_guide/topics/tasks/deploying_IBM_StoredIQ_on_Microsoft_Hyper-V.dita"><?ditaot usertext?>Use Microsoft Virtual Machine
                            Converter</xref></sli>
                    </sl>
                    <sl class="- topic/sl " xtrc="sl:2;75:25" xtrf="file:/C:/Users/patscully/Dropbox/Current%20Working%20Clients/Information%20Designs%20&amp;%20Development/project_2_ps/workspace/publications/deployment_and_configuration_guide/topics/tasks/deploying_IBM_StoredIQ_on_Microsoft_Hyper-V.dita">
